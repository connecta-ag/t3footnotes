{
  "name": "cag/t3footnotes",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension to add footnotes to TYPO3's integrated RTE CKEditor",
  "homepage": "https://github.com/connecta-ag/t3footnotes",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "gallery",
    "image",
    "slider",
    "lightbox",
    "extbase",
    "fluid"
  ],
  "authors": [
    {
      "name": "Jochen Rieger",
      "email": "j.rieger@connecta.ag"
    }
  ],
  "extra": {
    "typo3/cms": {
      "extension-key": "t3footnotes"
    }
  },
  "require": {
    "php": ">=7.0,<=8.1.99",
    "typo3/cms-core": ">=9.5.0,<=11.5.99"
  },
  "autoload": {
    "psr-4": {
      "CAG\\T3footnotes\\": "Classes/"
    }
  }
}
